/dts-v1/;

#include "gxbb_p200_1G_mibox3.dtsi"
/ {
	partitions: partitions{
		parts = <4>;
		part-0 = <&boot>;
		part-1 = <&eboot>;
		part-2 = <&system>;
		part-3 = <&data>;
		boot:boot{	// CoreELEC kernel
			pname = "boot";
			size = <0x0 0xb66000>;
			mask = <1>;
		};
		eboot:eboot{	// EmuELEC kernel
			pname = "eboot";
			size = <0x0 0xbec000>;
			mask = <1>;
		};
		system:system{	// HybridELEC system
			pname = "system";
			size = <0x0 0x385e6000>;
			mask = <1>;
		};
		data:data
		{
			pname = "data";
			size = <0xffffffff 0xffffffff>;
			mask = <4>;
		};
	};
};
