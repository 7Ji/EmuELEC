/dts-v1/;

#include "gxbb_p200_1G_mibox3.dtsi"
/ {
	partitions: partitions{
		parts = <5>;
		part-0 = <&boot>;
		part-1 = <&boot_emuelec>;
		part-2 = <&system>;
		part-3 = <&system_emuelec>;
		part-4 = <&data>;
		boot:boot{
			pname = "boot";
			size = <0x0 0xb6c000>;
			mask = <1>;
		};
		boot_emuelec:boot_emuelec{
			pname = "boot_emuelec";
			size = <0x0 0xc47000>;
			mask = <1>;
		};
		system:system{
			pname = "system";
			size = <0x0 0x9d2e000>;
			mask = <1>;
		};
		system_emuelec:system_emuelec{
			pname = "system_emuelec";
			size = <0x0 0x30876000>;
			mask = <1>;
		};
		data:data
		{
			pname = "data";
			size = <0xffffffff 0xffffffff>;
			mask = <4>;
		};
	};
};
